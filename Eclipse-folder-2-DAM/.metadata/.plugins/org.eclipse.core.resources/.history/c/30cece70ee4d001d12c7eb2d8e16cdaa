package productosApple;

import java.util.ArrayList;

public class AirPods {

    // Producto AirPods
    private String nombre;
    private double precio;
    private int stock;
    private String color;
    private String modelo;
    private ArrayList <String> compatibilidad = new ArrayList <String>();

    // Constructores
    public AirPods() {
        super();
    }

    public AirPods(String nombre, double precio, int stock, String color, String modelo, ArrayList<String> compatibilidad) {
        super();
        this.nombre = nombre;
        this.precio = precio;
        this.stock = stock;
        this.color = color;
        this.modelo = modelo;
        this.compatibilidad = compatibilidad;
    }



}
