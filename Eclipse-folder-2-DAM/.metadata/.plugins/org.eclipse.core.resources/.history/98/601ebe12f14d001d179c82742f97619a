package productosApple;

import java.util.ArrayList;

public class IWatch {
    
    
    // Producto IWatch
    private int id;
    private String nombre;
    private double precio;
    private int stock;
    private String color;
    private String modelo;
    private ArrayList <String> compatibilidad = new ArrayList <String>();

    // Constructores
    public IWatch() {
        super();
    }

    public IWatch(int id, String nombre, double precio, int stock, String color, String modelo, ArrayList<String> compatibilidad) {
        super();
        this.id = id;
        this.nombre = nombre;
        this.precio = precio;
        this.stock = stock;
        this.color = color;
        this.modelo = modelo;
        this.compatibilidad = compatibilidad;
    }    
    
    // Get y set

   

}
