package productosApple;

public class Mac {


		// Producto Mac
		private String nombre = "MacBook Pro";
		private double precio = 1299.99;
		private int stock = 5;
		private String procesador = "i5";
		private int ram = 8;
		private int almacenamiento = 256;
		private String color = "Gris";
		
		// Constructores

		public Mac(String nombre, double precio, int stock, String procesador, int ram, int almacenamiento, String color) {
			this.nombre = nombre;
			this.precio = precio;
			this.stock = stock;
			this.procesador = procesador;
			this.ram = ram;
			this.almacenamiento = almacenamiento;
			this.color = color;
		}



	}

}
